{
  "name": "react-undraw",
  "description": "React component for unDraw illustrations.",
  "version": "1.13.0",
  "keywords": [
    "undraw",
    "illustrations",
    "react",
    "reactjs",
    "svg"
  ],
  "author": {
    "name": "Justin Lettau",
    "email": "me@justinlettau.com",
    "url": "https://justinlettau.com"
  },
  "license": "MIT",
  "homepage": "https://justinlettau.github.io/react-undraw",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinlettau/react-undraw"
  },
  "bugs": {
    "url": "https://github.com/justinlettau/react-undraw/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js -c .eslintrc",
    "release": "standard-version",
    "refresh": "npm run download && npm run generate",
    "download": "node ./scripts/download.js",
    "generate": "npm run generate:components && npm run generate:mappings && npm run generate:types && npm run generate:readme",
    "generate:components": "node ./scripts/generate-components.js",
    "generate:mappings": "node ./scripts/generate-mappings.js",
    "generate:readme": "node ./scripts/generate-readme.js",
    "generate:barrel": "node ./scripts/generate-barrel.js",
    "generate:types": "node ./scripts/generate-types.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prepublishOnly": "npm run lib:build",
    "lib": "npm run lib:clean && npm run lib:build:esm &&npm run lib:build:cjs",
    "lib:clean": "rimraf dist",
    "lib:build:esm": "cross-env BABEL_ENV=esm NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "lib:build:cjs": "cross-env BABEL_ENV=cjs NODE_ENV=production babel src/lib --out-dir dist/cjs --copy-files",
    "format:all": "prettier --write **/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "types": "./undraw.d.ts",
  "files": [
    "dist",
    "undraw.d.ts"
  ],
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "dependencies": {
    "prop-types": "^15.5.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@justinlettau/eslint-config": "^0.1.2",
    "@svgr/core": "^4.1.0",
    "@svgr/plugin-jsx": "^4.1.0",
    "@svgr/plugin-prettier": "^4.0.3",
    "@svgr/plugin-svgo": "^4.0.3",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "cross-env": "^5.2.0",
    "dashify": "^2.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "fs-extra": "^7.0.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.2.0",
    "markdown-table": "^1.1.2",
    "ora": "^3.0.0",
    "pascal-case": "^2.0.1",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "^2.1.1",
    "rimraf": "^2.6.2",
    "standard-version": "^5.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
